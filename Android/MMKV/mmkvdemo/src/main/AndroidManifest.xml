<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.onyx.internal.mmkvdemo.MyApplication">
        <activity
            android:name="com.onyx.internal.mmkvdemo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.onyx.internal.mmkvdemo.MyService"
            android:enabled="true"
            android:exported="false"
            android:process=":mmkvdemo_svr">
            <intent-filter>
                <action android:name="com.onyx.internal.mmkvdemo.MyService"/>
            </intent-filter>
        </service>
        <service
            android:name="com.onyx.internal.mmkvdemo.MyService_1"
            android:enabled="true"
            android:exported="false"
            android:process=":mmkvdemo_svr_1">
            <intent-filter>
                <action android:name="com.onyx.internal.mmkvdemo.MyService_1"/>
            </intent-filter>
        </service>

        <provider android:name="com.onyx.internal.mmkvdemo.MultiProcessSharedPreferences"
            android:authorities=".MultiProcessSharedPreferences"
            android:process=".MultiProcessSharedPreferences"
            android:exported="false">
        </provider>

        <provider
            android:authorities="com.onyx.internal.mmkvdemo.MMKVContentProvider"
            android:name="com.onyx.internal.mmkv.MMKVContentProvider"
            android:exported="false">
        </provider>
    </application>

</manifest>